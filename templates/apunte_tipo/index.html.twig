{% extends 'base.html.twig' %}

{% block title %}ApunteTipo index{% endblock %}

{% block body %}
    <h1>ApunteTipo index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nombre</th>
                <th>FechaCreacion</th>
                <th>FechaActualizacion</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for apunte_tipo in apunte_tipos %}
            <tr>
                <td>{{ apunte_tipo.id }}</td>
                <td>{{ apunte_tipo.nombre }}</td>
                <td>{{ apunte_tipo.fechaCreacion ? apunte_tipo.fechaCreacion|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ apunte_tipo.fechaActualizacion ? apunte_tipo.fechaActualizacion|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('apunte_tipo_show', {'id': apunte_tipo.id}) }}">show</a>
                    <a href="{{ path('apunte_tipo_edit', {'id': apunte_tipo.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('apunte_tipo_new') }}">Create new</a>
{% endblock %}
